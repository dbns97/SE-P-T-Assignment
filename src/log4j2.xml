<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">

  
	<Appenders>
    
    	<File name="MyFile" fileName="logs/app.log">
      		<PatternLayout>
      			<pattern>[%-5level] %d{yyyy-MM-dd HH:mm:ss.SSS} [%t] %c{1} - %msg%n</pattern>
      		</PatternLayout>
    	</File>
    
	</Appenders>
  
	<Loggers>
    	<Root level="trace">
      		<AppenderRef ref="MyFile"/>
    	</Root>
  	</Loggers>
  
</Configuration>